worker:
  config:
    core:
      sources: ["custom", "pci", "usb"]
    sources:
      custom:
        - name: "intel-gpu"
          matchOn:
            - pciId:
                class: ["0300"]
                vendor: ["8086"]
        - name: "coral-tpu"
          matchOn:
            - usbId:
                vendor: ["1a6e", "18d1"]
